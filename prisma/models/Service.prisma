model Service {
  id                             String   @id @default(uuid())
  userId                         String
  user                           User     @relation(fields: [userId], references: [id])
  title                          String
  description                    String
  location                       String
  budget                         String
  startTime                      String
  toolsNeed                      Boolean
  file                           String?
  appliedServiceProvider         String[]
  confirmServiceProvider         String[]
  isCompletedFromServiceProvider Boolean  @default(false)
  isCompleteFromConsumer         Boolean  @default(false)
  isDeleteRequestToAdmin         Boolean  @default(false)
  isDeleted                      Boolean  @default(false)
  createdAt                      DateTime @default(now())
  updatedAt                      DateTime @updatedAt
}
