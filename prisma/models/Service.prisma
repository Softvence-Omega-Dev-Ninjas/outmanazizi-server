model Service {
  id                             String           @id @default(uuid())
  userId                         String
  user                           User             @relation(fields: [userId], references: [id])
  serviceName                    String
  title                          String
  subServices                    String
  description                    String
  location                       String
  budget                         String
  startTime                      String
  endTime                        String
  toolsNeed                      Boolean
  file                           String[]
  isCompletedFromServiceProvider Boolean          @default(false)
  isCompleteFromConsumer         Boolean          @default(false)
  isDeleteRequestToAdmin         Boolean          @default(false)
  isDeleted                      Boolean          @default(false)
  createdAt                      DateTime         @default(now())
  updatedAt                      DateTime         @updatedAt
  bids                           Bid[]
  assignedServiceProviderId      String?
  assignedServiceProvider        ServiceProvider? @relation(fields: [assignedServiceProviderId], references: [id])
  reviews                        Review[]
}
